pipeline{
agent any
environment{
    CC = "development"
}
    stages{
        stage('Build'){
            steps{
                echo 'Building the project'
                sh 'npm install'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing the project'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying the project'
            }
        }
    }